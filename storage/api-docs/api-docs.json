paths:
  /search:
    get:
      tags:
        - OPENSTREETMAP (saran utama)
      summary: GET kecamatan
      parameters:
        - name: User-Agent
          in: header
          schema:
            type: string
          example: yourname@example.com
        - name: q
          in: query
          schema:
            type: string
          example: Cimenyan,Bandung,Jawa Barat,Indonesia
        - name: format
          in: query
          schema:
            type: string
          example: json
        - name: limit
          in: query
          schema:
            type: integer
          example: '1'
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /admin/role:
    get:
      tags:
        - ROLE JABATAN > admin/kasubag > CRUD ROLES
      summary: tampil roles
      parameters:
        - name: Content-Type
          in: header
          schema:
            type: string
          example: application/json
        - name: Authorization
          in: header
          schema:
            type: string
          example: bearer {{token_auth_siakad}}
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
    post:
      tags:
        - ROLE JABATAN > admin/kasubag > CRUD ROLES
      summary: tambah roles
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                nama: Tambah Role
      parameters:
        - name: Content-Type
          in: header
          schema:
            type: string
          example: application/json
        - name: Authorization
          in: header
          schema:
            type: string
          example: bearer {{token_auth_siakad}}
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /admin/role/5:
    put:
      tags:
        - ROLE JABATAN > admin/kasubag > CRUD ROLES
      summary: edit roles
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                nama: Update Role
      parameters:
        - name: Content-Type
          in: header
          schema:
            type: string
          example: application/json
        - name: Authorization
          in: header
          schema:
            type: string
          example: bearer {{token_auth_siakad}}
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
    delete:
      tags:
        - ROLE JABATAN > admin/kasubag > CRUD ROLES
      summary: New Request
      parameters:
        - name: Content-Type
          in: header
          schema:
            type: string
          example: application/json
        - name: Authorization
          in: header
          schema:
            type: string
          example: bearer {{token_auth_siakad}}
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /admin/jenis-sk:
    get:
      tags:
        - ROLE JABATAN > admin/kasubag > CRUD Daftar Jenis SK
      summary: Tampil Jenis SK
      parameters:
        - name: Authorization
          in: header
          schema:
            type: string
          example: bearer {{token_auth_siakad}}
        - name: Accept
          in: header
          schema:
            type: string
          example: application/json
        - name: Content-Type
          in: header
          schema:
            type: string
          example: application/json
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
    post:
      tags:
        - ROLE JABATAN > admin/kasubag > CRUD Daftar Jenis SK
      summary: Tambah Jenis SK
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                kode: SK01
                jenis_sk: SK Kenaikan Pangkat
      parameters:
        - name: Content-Type
          in: header
          schema:
            type: string
          example: application/json
        - name: Accept
          in: header
          schema:
            type: string
          example: application/json
        - name: Authorization
          in: header
          schema:
            type: string
          example: bearer {{token_auth_siakad}}
      responses:
        '200':
          description: Successful response
